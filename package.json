{
  "name": "worksheet-be",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "migration:generate": "npm run build && sequelize-cli migration:generate --migrations-path src/database/migrations --name",
    "migration:create": "npm run build && sequelize-cli migration:create --migrations-path src/database/migrations --name",
    "migration:up": "npm run build && sequelize-cli db:migrate",
    "migration:up:one": "npm run build && sequelize-cli db:migrate --to",
    "migration:down:one": "npm run build && sequelize-cli db:migrate:undo --to",
    "migration:down:all": "npm run build && sequelize-cli db:migrate:undo:all",
    "migration:status": "npm run build && sequelize-cli db:migrate:status",
    "docker:dev": "docker-compose up mysql_dev -d",
    "docker:test": "docker-compose up mysql_test -d",
    "docker:env": "node -e \"require('dotenv').config(); console.log(process.env.NODE_ENV === 'test' ? 'Starting test database...' : 'Starting development database...'); require('child_process').execSync(process.env.NODE_ENV === 'test' ? 'npm run docker:test' : 'npm run docker:dev', {stdio: 'inherit'})\"",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v",
    "seed:generate": "sequelize-cli seed:generate --name",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo",
    "seed:undo:all": "sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3",
    "sequelize": "^6.37.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.8.6",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
